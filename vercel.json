{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/cart/add",
      "dest": "/index.js"
    },
    {
      "src": "/cart/edit",
      "dest": "/index.js"
    },
    {
      "src": "/cart/delete",
      "dest": "/index.js"
    },
    {
      "src": "/cart",
      "dest": "/index.js"
    },
    {
      "src": "/cart/total-quantity",
      "dest": "/index.js"
    },
    {
      "src": "/cart/total-price",
      "dest": "/index.js"
    }
  ]
}
